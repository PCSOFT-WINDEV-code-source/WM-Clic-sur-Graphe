     name : ExcentrePart
     procedure_id : 1860513668657287504
     type_code : 14
     code : |1+
      // Résumé : Excentre/Recentre la part cliquée
      // Syntaxe :
      //ExcentrePart (<nPosX>, <nPosY>)
      //
      // Paramètres :
      // 	nPosX : <indiquez ici le rôle de nPosX>
      // 	nPosY : <indiquez ici le rôle de nPosY>
      // Valeur de retour :
      // 	Aucune
      //
      
      PROCEDURE ExcentrePart( nPosX, nPosY )
      
      // Variables locales
      nIndicePart		est un entier
      
      
      // Récupère l'indice de la part qui vient d'être cliquée
      nIndicePart = grInfoXY(GRF_GrapheExcentre, grCatégorie, nPosX , nPosY )
      
      // Si l'indice est "hors bornes", ne rien faire
      SI nIndicePart = -1 ALORS RETOUR
      
      // Inverse l'état courant de la part dans le tableau (excentrée => centrée / centrée => excentrée)
      gtaPartsExcentrées[nIndicePart] = PAS gtaPartsExcentrées[nIndicePart]
      
      // Excentre (ou recentre) la part du graphe
      grSecteurPartExcentrée(GRF_GrapheExcentre, nIndicePart, cstExcentration * gtaPartsExcentrées[nIndicePart])
      
      // Redessine le graphe
      grDessine(GRF_GrapheExcentre)
